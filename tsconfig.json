{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "5.0",
    "importsNotUsedAsValues": "error",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "strict": true,
    "types": ["svelte"]
  },
  "include": ["src"]
}
